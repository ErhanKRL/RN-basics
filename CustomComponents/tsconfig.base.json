{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "lib": ["dom", "es2015"],
      "jsx": "react-native",
      "strict": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    }
  }
  